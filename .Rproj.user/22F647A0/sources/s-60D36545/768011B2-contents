\docType{data}
\name{case.dat}
\alias{case.dat}
\title{Data
This data is from Walker and Kadlec, "A Model for Simulating Phosphorus Concentrations in Waters & Soils Downstream of Everglades Stormwater Treatment Areas", prepared for US Dept of the Interior,  August 1996. available at http://www.wwwalker.net/epgm}
\format{An object of class \code{data.frame} with 12 rows and 17 columns.}
\usage{
case.dat

case.dat
}
\arguments{
\item{case.number}{Index value unique for each case}

\item{STA.Name}{The name of the Stormwater Treatment Area (STA) where this data originated.}

\item{Receiving.Area}{The area which this STA discharges.}

\item{Start.Discharge}{The year which this particular STA began discharge operations.}

\item{STA.outflow.TPconc}{Outflow total phosphorus concentration (in \\u03BCg L\u207B\u00B9) for this STA.}

\item{STA.outflow.vol}{Annual outflow discharge volume (in x10\u00B3 Acre-Feet Year\u207B\u00B9) for this STA.}

\item{FlowPath.width}{The width of the downstream flow path (in kilometers).}

\item{Hydroperiod}{Average hydroperiod (time above ground surface) of the downstream system (in percent).}

\item{Soil.Depth}{Depth of soil (in centimeters).}

\item{Soil.BulkDensity.intial}{The initial bulk density prior to dicharge of the soil downstream of the system (in g cm\u207B\u00B3).}

\item{Soil.TPConc.intial}{The initial total phosphorus concentration of soil prior to discharge downstream of the system (in mg kg\u207B\u00B9).}

\item{Vertical.soilTPGradient.intial}{The soil total phosphorus concentration gradient prior to dischage downstream of the system (in mg cm\u207B\u00B9 cm\u207B\u00B9).}

\item{Soil.BulkDensity.final}{The final bulk density after dischage of the soil downstream of the system (in g cm\u207B\u00B3).}

\item{PSettlingRate}{The phosphorus settling rate estimated from steady-state conditions (m Year\u207B\u00B9).}

\item{P.AtmoDep}{Phosphorus atmospheric depostition loading rate (in mg m\u207B\u00B2 Year\u207B\u00B9).}

\item{Rainfall}{Annual accumulated rainfall estimate (m Year\u207B\u00B9).}

\item{ET}{Annual evapotranspiration estimate (m Year\u207B\u00B9).}

\item{case.number}{Index value unique for each case}

\item{STA.Name}{The name of the Stormwater Treatment Area (STA) where this data originated.}

\item{Receiving.Area}{The area which this STA discharges.}

\item{Start.Discharge}{The year which this particular STA began discharge operations.}

\item{STA.outflow.TPconc}{Outflow total phosphorus concentration (in u03BCg L\u207B\u00B9) for this STA.}

\item{STA.outflow.vol}{Annual outflow discharge volume (in x10\u00B3 Acre-Feet Year\u207B\u00B9) for this STA.}

\item{FlowPath.width}{The width of the downstream flow path (in kilometers).}

\item{Hydroperiod}{Average hydroperiod (time above ground surface) of the downstream system (in percent).}

\item{Soil.Depth}{Depth of soil (in centimeters).}

\item{Soil.BulkDensity.intial}{The initial bulk density prior to dicharge of the soil downstream of the system (in g cm\u207B\u00B3).}

\item{Soil.TPConc.intial}{The initial total phosphorus concentration of soil prior to discharge downstream of the system (in mg kg\u207B\u00B9).}

\item{Vertical.soilTPGradient.intial}{The soil total phosphorus concentration gradient prior to dischage downstream of the system (in mg cm\u207B\u00B9 cm\u207B\u00B9).}

\item{Soil.BulkDensity.final}{The final bulk density after dischage of the soil downstream of the system (in g cm\u207B\u00B3).}

\item{PSettlingRate}{The phosphorus settling rate estimated from steady-state conditions (m Year\u207B\u00B9).}

\item{P.AtmoDep}{Phosphorus atmospheric depostition loading rate (in mg m\u207B\u00B2 Year\u207B\u00B9).}

\item{Rainfall}{Annual accumulated rainfall estimate (m Year\u207B\u00B9).}

\item{ET}{Annual evapotranspiration estimate (m Year\u207B\u00B9).}
}
